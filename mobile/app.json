{
  "expo": {
    "name": "Poseidon",
    "slug": "poseidon",
    "scheme": "poseidon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "plugins": [
      "expo-dev-client",
      "./plugins/withSolanaMWA"
    ],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0a1520"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.poseidon.trade"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0a1520"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.poseidon.trade"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "3c3d3ecb-2ce1-4b9c-938c-e8c005a7bd38"
      }
    }
  }
}
