{
  "compilerOptions": {
    "strict": false,
    "target": "ESNext",
    "module": "commonjs",
    "jsx": "react-native",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "noEmit": true
  },
  "exclude": ["node_modules", "babel.config.js"]
}
